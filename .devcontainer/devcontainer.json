{
  "name": "Grand Pope",
  "dockerComposeFile": "../docker-compose.dev.yml",
  "service": "Sanctuaire",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8000],
  "customizations": {
    "vscode": {
        "settings": {
            "terminal.integrated.defaultProfile.linux": "sh"
        }
    }
  },
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=bind"
  ],
  "overrideCommand": false
}
